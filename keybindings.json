// Place your key bindings in this file to override the defaultsauto[]
[
    {
        "key": "ctrl+; b",
        "command": "workbench.action.tasks.runTask",
        "args": "Clover: Tap block"
    },
    {
        "key": "ctrl+; shift+b",
        "command": "workbench.action.tasks.runTask",
        "args": "Clover: Tap top block"
    },
    {
        "key": "ctrl+; c",
        "command": "clover.break-evaluation"
    },
    {
        "key": "ctrl+; d",
        "command": "clover.doc-for-var"
    },
    {
        "key": "ctrl+; shift+d",
        "command": "workbench.action.tasks.runTask",
        "args": "Clover: Tap def var"
    },
    {
        "key": "ctrl+; e",
        "command": "clover.disconnect"
    },
    {
        "key": "ctrl+; f",
        "command": "clover.load-file"
    },
    {
        "key": "ctrl+; j",
        "command": "workbench.action.tasks.runTask",
        "args": "Clover: Tap javadoc"
    },
    {
        "key": "ctrl+; k",
        "command": "clover.clear-console"
    },
    {
        "key": "ctrl+; shift+k",
        "command": "workbench.action.tasks.runTask",
        "args": "Clover: Portal clear"
    },
    {
        "key": "ctrl+; shift+p",
        "command": "workbench.action.tasks.runTask",
        "args": "Clover: Portal start"
    },
    {
        "key": "ctrl+; n",
        "command": "workbench.action.tasks.runTask",
        "args": "Clover: Tap ns"
    },
    {
        "key": "ctrl+; r",
        "command": "workbench.action.tasks.runTask",
        "args": "Clover: Tap remove ns"
    },
    {
        "key": "ctrl+; shift+r",
        "command": "workbench.action.tasks.runTask",
        "args": "Clover: Tap reload all ns"
    },
    {
        "key": "ctrl+; s",
        "command": "workbench.action.tasks.runTask",
        "args": "Clover: Tap selection"
    },
    {
        "key": "ctrl+; shift+s",
        "command": "clover.source-for-var"
    },
    {
        "key": "ctrl+; t",
        "command": "workbench.action.tasks.runTask",
        "args": "Clover: Tap run current test"
    },
    {
      "key": "ctrl+; shift+t",
      "command": "workbench.action.tasks.runTask",
      "args": "Clover: Tap block as tree"
    },
    {
        "key": "ctrl+; v",
        "command": "workbench.action.tasks.runTask",
        "args": "Clover: Tap var"
    },
    {
        "key": "ctrl+; x",
        "command": "workbench.action.tasks.runTask",
        "args": "Clover: Tap run tests"
    },
    {
        "key": "ctrl+; shift+x",
        "command": "workbench.action.tasks.runTask",
        "args": "Clover: Tap run side tests"
    },
    {
        "key": "ctrl+; y",
        "command": "clover.connectSocketRepl"
    },
    {
        "key": "ctrl+; shift+/",
        "command": "workbench.action.tasks.runTask",
        "args": "Clover: Tap doc var"
    },
    {
      "key": "ctrl+; shift+a",
      "command": "workbench.action.tasks.runTask",
      "args": "Clover: Tap add libs"
  },
  {
        "key": "ctrl+; .",
        "command": "clover.go-to-var-definition"
    },
    {
      "key": "ctrl+right",
      "command": "paredit.forwardSexp",
      "when": "calva:keybindingsEnabled && editorTextFocus && editorLangId == 'clojure' && paredit:keyMap =~ /original|strict/"
    },
    {
      "key": "alt+right",
      "command": "cursorWordRight"
    },
    {
      "key": "ctrl+left",
      "command": "paredit.backwardSexp",
      "when": "calva:keybindingsEnabled && editorTextFocus && editorLangId == 'clojure' && paredit:keyMap =~ /original|strict/"
    },
    {
      "key": "alt+left",
      "command": "cursorWordLeft"
    },
    {
      "key": "ctrl+alt+e ctrl+alt+j",
      "command": "workbench.action.joinTwoGroups"
    },
    {
      "key": "ctrl+j",
      "command": "editor.action.joinLines"
    },
    {
      "key": "ctrl+alt+a ctrl+alt+b",
      "command": "atlascode.views.bb.pullrequestsTreeView.focus"
    },
    {
      "key": "ctrl+alt+a ctrl+alt+j",
      "command": "atlascode.views.jira.customJql.focus"
    },
    {
      "key": "ctrl+t ctrl+f",
      "command": "workbench.action.terminal.focus"
    },
    {
      "key": "ctrl+t ctrl+t",
      "command": "workbench.action.quickOpenTerm"
    },
    {
      "key": "ctrl+u ctrl+u",
      "command": "editor.action.transformToUppercase"
    },
    {
      "key": "ctrl+l ctrl+l",
      "command": "editor.action.transformToLowercase"
    }
]